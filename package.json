{
  "name": "version-lite",
  "version": "0.0.1",
  "description": "A lightweight JavaScript library for quick version validation and comparison.",
  "type": "module",
  "scripts": {
    "commit": "git add . && cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "rollup --config build/rollup.config.js --watch",
    "build": "rollup --config build/rollup.config.js",
    "lint": "eslint --cache --no-error-on-unmatched-pattern src/**/*.{j,t}s{,x}",
    "lint:fix": "eslint --cache --no-error-on-unmatched-pattern --fix src/**/*.{j,t}s{,x}",
    "test": "jest"
  },
  "author": "Michael Chen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@changesets/cli": "^2.27.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@slothkit/eslint-config": "^1.3.0",
    "@types/jest": "^29.5.12",
    "@types/jest-dev-server": "^5.0.3",
    "@types/node": "^20.12.3",
    "commitizen": "^4.3.0",
    "core-js": "^3.36.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "jest-dev-server": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup": "^4.18.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=16"
  }
}
